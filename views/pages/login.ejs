<!-- views/pages/login.ejs -->

<!DOCTYPE html>
<html lang="en">

<head>
    <% include ../partials/head %>
</head>

<body class="container">

    <header>
        <% include ../partials/header %>
    </header>

    <main>
        <div class="container row">
            <div class="col-md-6">
        <form>
            <div class="form-group" id="usernameLogForm">
                <label for="usernameLog">Username</label>
                <input type="text" class="form-control" id="usernameLog" aria-describedby="emailHelp"
                    placeholder="Username">
            </div>
            <div class="form-group" id="passLogForm">
                <label for="exampleInputPassword1">Password</label>
                <input type="password" class="form-control" id="passLog" autocomplete="password" placeholder="Password">
            </div>
            <button type="submit" id="logInBtn" class="btn btn-warning btn-lg">Submit</button>
        </form>
    </div>
</div>
    </main>



    <footer>
        <% include ../partials/footer %>
    </footer>

</body>

</html>
<html lang="en-US">

<head>
  <% include ../partials/head.ejs %>
  <title>Sign Up</title>
</head>

<body>

  <% include ../partials/header.ejs %>
  <div>
    <form id="usernameSign_up_form" action="get">
      <div class="alignGridWrapper">
        <div>
          <label for="exampleInputEmail1">Username</label>
          <input type="text" class="name" id="usernameSign_up" aria-describedby="emailHelp" placeholder="Username">
        </div>
        <div id="nameSign_up_form">
          <label for="exampleInputEmail1">Name</label>
          <input type="text" id="nameSign_up" aria-describedby="emailHelp" placeholder="Name">
        </div>
        <div id="emailSign_up_form">
          <label for="exampleInputEmail1">Email Address</label>
          <input type="email" id="emailSign_up" aria-describedby="emailHelp" placeholder="Enter Email">
        </div>
        <div id="passwordSign_up_form">
          <label for="exampleInputPassword1">Password</label>
          <input type="password" autocomplete="password" id="passwordSign_up" placeholder="Password">
        </div>
        <div class="center">
          <button type="submit" id="signUpBtn">Submit <i class="fab fa-forumbee"></i></button>
        </div>
      </div>
    </form>
  </div>
 
  </footer>
</body>
</html>